# OpenRouter API key (https://openrouter.ai/keys)
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Firecrawl API key (https://firecrawl.dev)
FIRECRAWL_API_KEY=your_firecrawl_api_key_here

# LangSmith observability (https://smith.langchain.com)
# Set LANGSMITH_TRACING=true to enable tracing; unset or false to disable
LANGSMITH_TRACING=true
LANGSMITH_API_KEY=your_langsmith_api_key_here
# Optional: project name for grouping traces (default: "default")
# LANGSMITH_PROJECT=broker
# Optional: for non-serverless, reduces latency (default: true in langsmith)
# LANGCHAIN_CALLBACKS_BACKGROUND=true
